/**
 * File:
 *   tmp_alice_part.scr
 * Summary:
 *   SCR Agent for reading yast1/alice partition plans
 * Access:
 *   read
 * Authors:
 *   Unknown <yast2-hacker@suse.de>
 * See:
 *
 *   eth0 10.70.123.14    255.255.0.0 10.70.123.255
 *   anyagent
 *   libscr
 */
.tmp.alice.routes
`ag_anyagent(
    Description(
    ``(File("/var/lib/autoinstall/tmp/alice_routes")),  // real file name
    "#\n",                      // comment
    true,                      // read-only?
    ``(List(
        Tuple(
		Optional(Whitespace()),
        `destination(String("^ \t\n")),
        Whitespace(),
        `gateway(String("^ \t\n")),
        Whitespace(),
        `netmask(String("^ \t\n")),
        Optional(Whitespace()),
        Optional(`device(String("^ \t\n")))
        ),
    "\n"
    ))
    )
)


