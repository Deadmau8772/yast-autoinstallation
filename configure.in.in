## YCP module configure.in.in

## Initialize
@YAST2-INIT-COMMON@
@YAST2-INIT-YCP@

if test "${sysconfdir}" = '${prefix}/etc'; then
    sysconfdir=/etc
fi

icondir=/usr/share/icons
AC_SUBST(icondir)

## some common checks
@YAST2-CHECKS-COMMON@
@YAST2-CHECKS-YCP@

## and generate the output...
@YAST2-OUTPUT@
