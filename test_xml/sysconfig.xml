<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <sysconfig config:type="list">
    <sysconfig_entry>
      <sysconfig_key>MAX_RPMDB_BACKUPS</sysconfig_key>
      <sysconfig_path>/etc/sysconfig/backup</sysconfig_path>
      <sysconfig_value>15</sysconfig_value>
    </sysconfig_entry>
    <sysconfig_entry>
      <sysconfig_key>NFS4_SUPPORT</sysconfig_key>
      <sysconfig_path>/etc/sysconfig/nfs</sysconfig_path>
      <sysconfig_value>no</sysconfig_value>
    </sysconfig_entry>
  </sysconfig>
</profile>
