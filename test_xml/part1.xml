<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
<!--
use all
-->
<runlevel>
 <default>3</default>
 <services config:type="list">
  <service>
   <service_name>rpcbind</service_name>
   <service_status>enable</service_status>
  </service>
 </services>
</runlevel>

  <firstboot>
      <firstboot_enabled config:type="boolean">false</firstboot_enabled>
  </firstboot>

  <nis>
    <nis_broadcast config:type="boolean">false</nis_broadcast>
    <nis_broken_server config:type="boolean">false</nis_broken_server>
    <nis_domain>suse.de</nis_domain>
    <nis_local_only config:type="boolean">false</nis_local_only>
    <nis_servers config:type="list">
      <nis_server>10.10.0.1</nis_server>
    </nis_servers>
    <start_autofs config:type="boolean">true</start_autofs>
    <start_nis config:type="boolean">false</start_nis>
  </nis>

<scripts>
    <chroot-scripts config:type="list">
      <script>
        <feedback config:type="boolean">true</feedback>
        <debug config:type="boolean">true</debug>
        <chrooted config:type="boolean">true</chrooted>
        <interpreter>shell</interpreter>
        <source><![CDATA[
#echo "rm /var/lib/YaST2/runme_at_boot" >> /etc/init.d/boot.local
#touch /var/lib/YaST2/reconfig_system
        ]]></source>
      </script>
    </chroot-scripts>
    <init-scripts config:type="list">
      <script>
        <source><![CDATA[
echo "init-script has run" > /tmp/ay_init_script
]]></source>
      </script>
      <script>
        <location>nfs://10.10.0.162/space/exports/init-script-test.sh</location>
        <filename>remote-init</filename>
      </script>
    </init-scripts>
    <pre-scripts config:type="list">
      <script>
        <interpreter>shell</interpreter>
        <source><![CDATA[
echo "pre-script has run" > /tmp/ay_pre_script
mkdir /etc/zypp
echo "[main]
solver.onlyRequires = true
" >  /etc/zypp/zypp.conf
/tmp/env >> /tmp/ay_pre_script
]]></source>
      </script>
    </pre-scripts>
    <post-scripts config:type="list">
      <script>
        <interpreter>shell</interpreter>
        <network_needed config:type="boolean">true</network_needed>
        <source><![CDATA[
{
    set -x
    rpm -Uvh /tmp/yast2-network-2.17.170-0.5.7.i586.rpm
    set +x
} &>/tmp/update_autoyast.log
]]></source>
      </script>
    </post-scripts>
</scripts>
