      <shell>/bin/bash</shell>
      <uid>0</uid>
      <user_password>$6$0TfzIJLtYubi$13y4.XJeO082KRmhh8xSPw/ijYXbufIZpxqIB2/lwSVuPvsmol4kNTzO3vTVJyOucxGSSISE1DOX8lnvAybkH/</user_password>
